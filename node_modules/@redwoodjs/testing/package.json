{
  "name": "@redwoodjs/testing",
  "description": "Tools, wrappers and configuration for testing a Redwood project.",
  "version": "0.30.0",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@redwoodjs/auth": "^0.30.0",
    "@redwoodjs/internal": "^0.30.0",
    "@redwoodjs/router": "^0.30.0",
    "@redwoodjs/web": "^0.30.0",
    "@testing-library/react": "11.2.2",
    "@types/react": "17.0.3",
    "msw": "^0.21.2"
  },
  "scripts": {
    "build": "yarn build:js && yarn build:types",
    "prepublishOnly": "yarn cross-env NODE_ENV=production yarn build",
    "build:js": "babel src -d dist --extensions \".js,.ts,.tsx\"",
    "build:types": "ttsc --build --verbose",
    "build:watch": "nodemon --watch src --ext 'js,ts,tsx' --ignore dist --exec 'yarn build'",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "gitHead": "6d406412d2b1e386879653af0dd0aebf540d4090"
}
