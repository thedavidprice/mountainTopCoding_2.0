{
  "name": "@redwoodjs/api-server",
  "description": "Redwood's HTTP server for Serverless Functions",
  "version": "0.36.4",
  "bin": {
    "api-server": "./dist/index.js",
    "rw-api-server": "./dist/index.js",
    "rw-api-server-watch": "./dist/watch.js"
  },
  "files": [
    "dist"
  ],
  "main": "dist/handler",
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "4.1.1",
    "body-parser": "1.19.0",
    "chokidar": "3.5.2",
    "dotenv-defaults": "2.0.2",
    "express": "4.17.1",
    "fast-glob": "3.2.5",
    "http-proxy-middleware": "2.0.1",
    "lodash.escape": "4.0.1",
    "morgan": "1.10.0",
    "qs": "6.9.3",
    "yargs": "16.2.0",
    "youch": "2.2.2",
    "youch-terminal": "1.1.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.81",
    "@types/express": "4.17.13",
    "@types/lodash.escape": "4.0.6",
    "@types/morgan": "1.9.3",
    "@types/qs": "6.9.7",
    "@types/stack-trace": "0.0.29"
  },
  "scripts": {
    "build": "yarn build:js",
    "prepublishOnly": "yarn cross-env NODE_ENV=production yarn build",
    "build:js": "babel src -d dist --extensions \".js,.ts,.tsx\"",
    "build:watch": "nodemon --watch src --ext \"js,ts,tsx\" --ignore dist --exec \"yarn build && yarn fix:permissions\"",
    "fix:permissions": "chmod +x dist/index.js; chmod +x dist/watch.js"
  },
  "gitHead": "27afdf5bf06cdaa539215688c2cb580f5111b5af"
}
