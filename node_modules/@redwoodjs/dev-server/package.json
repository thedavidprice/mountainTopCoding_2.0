{
  "name": "@redwoodjs/dev-server",
  "description": "Redwood's HTTP server for serverless Functions",
  "version": "0.30.0",
  "bin": {
    "dev-server": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
