{
  "name": "@redwoodjs/dev-server",
  "description": "Redwood's HTTP server for serverless Functions",
  "version": "0.35.2",
  "bin": {
    "dev-server": "./dist/main.js"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/register": "7.13.16",
    "@redwoodjs/internal": "0.35.2",
    "body-parser": "1.19.0",
    "chokidar": "3.5.2",
    "express": "4.17.1",
    "morgan": "1.10.0",
    "qs": "6.9.3",
    "require-dir": "1.2.0",
    "yargs": "16.0.3",
    "youch": "2.2.2",
    "youch-terminal": "1.1.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.77",
    "@types/express": "4.17.12",
    "@types/morgan": "1.9.2",
    "@types/qs": "6.9.6",
    "@types/require-dir": "1.0.1",
    "@types/stack-trace": "0.0.29"
  },
  "scripts": {
    "build": "yarn build:js",
    "prepublishOnly": "yarn cross-env NODE_ENV=production yarn build",
    "build:js": "babel src -d dist --extensions \".js,.ts,.tsx\"",
    "build:watch": "nodemon --watch src --ext \"js,ts,tsx\" --ignore dist --exec \"yarn build && yarn fix:permissions\"",
