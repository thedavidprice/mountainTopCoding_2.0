    "build:types": "tsc --build --verbose",
    "build:watch": "nodemon --watch src --ext \"js,ts,tsx\" --ignore dist --exec \"yarn build\"",
    "fix:permissions": "chmod +x dist/generate/generate.js dist/generate/watch.js",
    "test": "jest",
    "test:watch": "yarn test --watch"
