{
  "name": "@redwoodjs/internal",
  "version": "0.35.1",
  "files": [
    "dist"
  ],
  "bin": {
    "rw-gen": "./dist/generate/generate.js",
    "rw-gen-watch": "./dist/generate/watch.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "7.14.4",
    "@babel/plugin-transform-typescript": "7.14.6",
    "@babel/runtime-corejs3": "7.14.0",
    "@babel/traverse": "7.14.2",
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/core": "1.17.10",
    "@graphql-codegen/schema-ast": "1.18.3",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-operations": "1.18.3",
    "@graphql-codegen/typescript-react-apollo": "2.3.0",
    "@graphql-codegen/typescript-resolvers": "1.19.5",
    "deepmerge": "4.2.2",
    "fast-glob": "3.2.5",
    "findup-sync": "4.0.0",
    "glob": "7.1.7",
    "graphql": "15.5.1",
    "kill-port": "1.6.1",
    "prettier": "2.3.0",
    "toml": "3.0.0"
  },
  "devDependencies": {
    "@types/findup-sync": "4.0.0",
    "graphql-tag": "2.12.5"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/fixtures/"
    ]
  },
  "scripts": {
    "build": "yarn build:js && yarn build:types",
    "prepublishOnly": "yarn cross-env NODE_ENV=production yarn build",
    "build:clean-dist": "rimraf 'dist/**/*/__tests__'",
    "build:js": "babel src -d dist --extensions \".js,.ts,.tsx\" --copy-files --no-copy-ignored && yarn build:clean-dist",
    "build:types": "ttsc --build --verbose",
    "build:watch": "nodemon --watch src --ext \"js,ts,tsx\" --ignore dist --exec \"yarn build\"",
    "fix:permissions": "chmod +x dist/generate/generate.js dist/generate/watch.js",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "gitHead": "770d6a13ee783960dcd15efc85e14fbecc9b7d98"
}
